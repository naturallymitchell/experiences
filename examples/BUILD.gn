group("examples") {
  deps = [
    "hello_experiences",
    "localized_flutter_app",
    "spinning_cube",
  ]
}

group("tests") {
  testonly = true

  public_deps = [
    ":dart_unittests",
  ]
}

group("dart_unittests") {
  testonly = true

  deps = [
    "localized_flutter_app:localized_flutter_app_unittests($host_toolchain)",
    "hello_experiences:hello_experiences_unittests($host_toolchain)",
  ]
}
