{
    program: {
        binary: "bin/rust_test_adapter",
        args: [
            "/pkg/test/workstation_session_bin_test",
        ]
    },
    expose: [
        {
            protocol: "/svc/fuchsia.test.Suite",
            from: "self",
       },
    ],
    use: [
        { runner: "elf" },
        {
            protocol: "/svc/fuchsia.sys2.Realm",
            from: "realm",
        },
        {
            protocol: [
                "/svc/fuchsia.process.Launcher",
                "/svc/fuchsia.logger.LogSink",
            ]
        },
    ],
}
